#!nsh
#
# @name THOR
#
# @type  Tri-Mode VTOL
# @class VTOL
#
# @output MAIN1 motor left
# @output MAIN2 motor right
# @output MAIN4 collective
# @output MAIN5 cyclic left
# @output MAIN6 cyclic right

# @maintainer Low Jun En <junen_low@sutd.edu.sg>
#
# TODO: PARAM SET in the if loops seems to not be registering (not completely at least).


###############

sh /etc/init.d/rc.vtol_defaults

if [ $AUTOCNF == yes ]
then
	param set MC_ROLL_P 6.0
	param set MC_ROLLRATE_P 0.12
	param set MC_ROLLRATE_I 0.002
	param set MC_ROLLRATE_D 0.003
	param set MC_ROLLRATE_FF 0.0
	param set MC_PITCH_P 4.5
	param set MC_PITCHRATE_P 0.3
	param set MC_PITCHRATE_I 0.002
	param set MC_PITCHRATE_D 0.003
	param set MC_PITCHRATE_FF 0.0
	param set MC_YAW_P 3.8
	param set MC_YAW_FF 0.5
	param set MC_YAWRATE_P 0.22
	param set MC_YAWRATE_I 0.02
	param set MC_YAWRATE_D 0.0
	param set MC_YAWRATE_FF 0.0

	param set VT_MOT_COUNT 2
	param set VT_IDLE_PWM_MC  1080
	param set VT_TYPE 3
	param set MAV_TYPE 19
	param set VT_ELEV_MC_LOCK 0

	param set VT_TYPE 3
	param set VT_THOR_RPS_TARG 31.0
	param set VT_THOR_RPS_P    0.12
	param set VT_THOR_RPS_I    0.04
	param set VT_THOR_CYC_P    0.10
	param set VT_THOR_COLL_P   0.50
	param set VT_THOR_THR_TRIG 0.35
	param set VT_THOR_DIR_TRIG 0.30
fi

set MIXER THOR

set PWM_OUT 123456
